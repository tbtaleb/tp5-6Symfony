{% extends 'base.html.twig' %}

{% block title %}Institue index{% endblock %}

{% block body %}
    <h1>Institue index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Institue_id</th>
                <th>Nomi</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for institue in institues %}
            <tr>
                <td>{{ institue.id }}</td>
                <td>{{ institue.institueId }}</td>
                <td>{{ institue.nomi }}</td>
                <td>
                    <a href="{{ path('app_institue_show', {'id': institue.id}) }}">show</a>
                    <a href="{{ path('app_institue_edit', {'id': institue.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_institue_new') }}">Create new</a>
{% endblock %}
